{
    "inputs": [
        {
            "command-line-flag": "--expectedVolume",
            "name": "expectedVolume",
            "default-value": "50",
            "required": true,
            "type": "number",
            "description": "The approximate volume of the object, in mL."
        },
        {
            "command-line-flag": "--intensityHomogeneity",
            "name": "intensityHomogeneity",
            "default-value": "0.6",
            "required": true,
            "type": "number",
            "description": "What is the homogeneity of intensity within the object? Given constant intensity at 1.0 score and extreme fluctuating intensity at 0."
        },
        {
            "command-line-flag": "--curvatureWeight",
            "name": "curvatureWeight",
            "default-value": "0.5",
            "required": true,
            "type": "number",
            "description": "Given sphere 1.0 score and extreme rough bounday/surface 0 score, what is the expected smoothness of the object?"
        },
        {
            "command-line-flag": "--labelValue",
            "name": "labelValue",
            "default-value": "1",
            "required": true,
            "type": "number",
            "description": "Label value of the output image"
        },
        {
            "command-line-flag": "--maxRunningTime",
            "name": "maxRunningTime",
            "default-value": "10",
            "required": true,
            "type": "number",
            "description": "The program will stop if this time is reached."
        },
        {
            "required": true,
            "type": "string",
            "name": "originalImageFileName",
            "default-value": "",
            "description": "Original image to be segmented"
        },
        {
            "required": true,
            "type": "string",
            "name": "labelImageFileName",
            "default-value": "",
            "description": "Label image for initialization"
        },
        {
            "required": true,
            "type": "string",
            "name": "segmentedImageFileName",
            "default-value": "",
            "description": "Segmented image"
        }
    ],
    "description": "Active contour segmentation using robust statistic.",
    "image": "quay.io/fedorov/slicerdockers:4.8.1",
    "info-url": "http://wiki.slicer.org/slicerWiki/index.php/Documentation/Nightly/Modules/RobustStatisticsSegmenter",
    "name": "Robust Statistics Segmenter",
    "command-line": "/opt/slicer/Slicer --launch RobustStatisticsSegmenter #expectedVolume# #intensityHomogeneity# #curvatureWeight# #labelValue# #maxRunningTime# -- #originalImageFileName# #labelImageFileName# #segmentedImageFileName#",
    "xnat": [],
    "label": "Robust Statistics Segmenter",
    "version": "1.0",
    "schema-version": "1.0",
    "type": "docker"
}